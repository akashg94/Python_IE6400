   "source": [
    "## Set Operations"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {
    "id": "5_DcM9_Nl3Fc"
   },
   "outputs": [],
   "source": [
    "A = {1, 2, 3, 4}\n",
    "B = {3, 4, 5, 6}\n",
    "\n",
    "# Union\n",
    "union_set = A | B\n",
    "\n",
    "# Intersection\n",
    "intersection_set = A & B\n",
    "\n",
    "# Difference\n",
    "difference_set = A - B\n",
    "\n",
    "# Symmetric Difference\n",
    "symmetric_diff = A ^ B\n",
    "\n",
    "# Subset and Superset Check\n",
    "is_subset = A.issubset(B)\n",
    "is_superset = A.issuperset(B)\n"
   ]
